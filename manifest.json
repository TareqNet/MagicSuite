{
	"name": "Magic Suite",
	"description": "Magic Suite is small tool that help you to do something for any link, images and other.",
	"version": "1.1.0",
	"manifest_version": 2,
	"homepage_url": "http://labs.tareq.tk/ChromeEx/MagicSuite",
	"update_url": "https://clients2.google.com/service/update2/crx",
	"permissions":
	[
		"tabs",
		"contextMenus"
	],
	"background":
	{
		"scripts": ["script.js"]
	},
	"browser_action":
	{
		"default_icon": "Icons/256.png",
		"default_popup": "popup.html"
	},
	"icons":
	{
    	"128": "Icons/256.png"
    }
}